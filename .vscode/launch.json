    "version": "0.2.0",
    "configurations": [
        {
            "name": "legacy_compiler",
            "request": "launch",
            "type": "dart",
            "args": [
                "-v",
                "-f",
                "assets/testing/sample.ap"
            ],
            "cwd": "${workspaceFolder}/legacy_compiler"
        },
        {
            "name": "legacy_compiler (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "-v",
                "-f",
                "assets/testing/sample.ap"
            ],
            "cwd": "${workspaceFolder}/legacy_compiler"
        },
        {
            "name": "legacy_compiler (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "-v",
                "-f",
                "assets/testing/sample.ap"
            ],
            "cwd": "${workspaceFolder}/legacy_compiler"
        }
    ]
}
